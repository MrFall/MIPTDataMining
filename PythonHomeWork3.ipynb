    "\t\tpivot = arr[0]\n",
    "\t\tfor i in arr:\n",
    "\t\t\tif i < pivot:\n",
    "\t\t\t\tm_1.append(i)\n",
    "\t\t\telif i == pivot:\n",
    "\t\t\t\tm_2.append(i)\n",
    "\t\t\telif i > pivot:\n",
    "\t\t\t\tm_3.append(i)\n",
    "\t\treturn Qsort(m_1)+ m_2 + Qsort(m_3)\n",
    "\telse:\n",
    "\t\treturn arr"
